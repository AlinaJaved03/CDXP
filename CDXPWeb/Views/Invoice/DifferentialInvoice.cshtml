        String.prototype.FormatRow = function () {
            try {
                var content = this;
                for (var i = 0; i < arguments.length; i++) {
                    var replacement = '{' + i + '}';
                    content = content.replace(replacement, arguments[i]);
                }
                return content;
            }
            catch (e) {
                console.log("Error occurred in FormatRow " + e.message);
